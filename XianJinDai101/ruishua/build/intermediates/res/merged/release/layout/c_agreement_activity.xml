<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/grey_bg"
	android:orientation="vertical">
	<include
		android:id="@+id/c_credit_home_header"
		layout="@layout/c_widget_title"/>
	<com.zhy.autolayout.AutoLinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="#eef1f3"
		android:orientation="vertical">
		<!--委托代扣协议 -->
		<com.zhy.autolayout.AutoRelativeLayout
			android:id="@+id/rl_agreement_daikou"
			android:layout_width="match_parent"
			android:layout_height="150px"
			android:layout_marginTop="60px"
			android:background="@drawable/selector_user_action_bg"
			android:clickable="true"
			android:visibility="gone"
		>
			<TextView
				style="@style/textViewStyle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_marginLeft="40px"
				android:text="委托扣款服务协议"/>

			<ImageView
				android:layout_width="25px"
				android:layout_height="54px"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="46px"
				android:scaleType="centerInside"
				android:src="@drawable/icon_user_jiantou"/>

		</com.zhy.autolayout.AutoRelativeLayout>
		<View
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:background="#e0e0e0"
		/>
		<!--瑞易贷服务协议-->
		<com.zhy.autolayout.AutoRelativeLayout
			android:id="@+id/rl_agreement_service"
			android:layout_width="match_parent"
			android:layout_height="150px"
			android:background="@drawable/selector_user_action_bg"
			android:clickable="true"
		>
			<TextView
				style="@style/textViewStyle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_marginLeft="40px"
				android:text="信用咨询及管理服务协议"/>

			<ImageView
				android:layout_width="25px"
				android:layout_height="54px"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="46px"
				android:scaleType="centerInside"
				android:src="@drawable/icon_user_jiantou"/>
		</com.zhy.autolayout.AutoRelativeLayout>
		<View
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:background="#e0e0e0"
		/>
		<!--瑞易贷借款协议-->
		<com.zhy.autolayout.AutoRelativeLayout
			android:id="@+id/rl_agreement_borrow"
			android:layout_width="match_parent"
			android:layout_height="150px"
			android:background="@drawable/selector_user_action_bg"
			android:clickable="true"
		>
			<TextView
				android:id="@+id/tv_brcontractName"
				style="@style/textViewStyle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_marginLeft="40px"
				android:text=""/>

			<ImageView
				android:layout_width="25px"
				android:layout_height="54px"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="46px"
				android:scaleType="centerInside"
				android:src="@drawable/icon_user_jiantou"/>
		</com.zhy.autolayout.AutoRelativeLayout>
		<View
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:background="#e0e0e0"
			/>
<!--		&lt;!&ndash;委托扣款服务协议&ndash;&gt;
		<com.zhy.autolayout.AutoRelativeLayout
			android:id="@+id/rl_agreement_url"
			android:layout_width="match_parent"
			android:layout_height="150px"
			android:background="@drawable/selector_user_action_bg"
			android:clickable="true"
			>
			<TextView
				style="@style/textViewStyle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_marginLeft="40px"
				android:text="委托扣款服务协议"/>

			<ImageView
				android:layout_width="25px"
				android:layout_height="54px"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="46px"
				android:scaleType="centerInside"
				android:src="@drawable/icon_user_jiantou"/>
		</com.zhy.autolayout.AutoRelativeLayout>-->
	</com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>
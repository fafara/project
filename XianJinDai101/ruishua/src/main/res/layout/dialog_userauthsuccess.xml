<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="1260px"
    android:layout_height="wrap_content"
    xmlns:custom="http://schemas.android.com/tools"
    android:background="@drawable/dialog_userauthsuccess_bakshape"
    android:orientation="vertical">
<ImageView
    android:id="@+id/dialogg_userauth_resultimg"
    android:layout_width="226px"
    android:layout_height="225px"
    android:layout_gravity="center_horizontal"
    android:layout_marginTop="90px"/>

    <TextView
        android:id="@+id/main_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:gravity="center_horizontal"
        android:layout_marginTop="90px"/>
    <TextView
        android:id="@+id/second_content_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_marginTop="142px"
        android:textSize="@dimen/bodylittletextsize"
        android:textColor="@color/threeblack"/>
    <LinearLayout
        android:layout_width="600px"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="150px"
        android:layout_marginBottom="135px"
        android:layout_marginLeft="200px"
        android:layout_marginRight="200px"
        android:orientation="vertical">

        <com.rey.material.widget.Button
            android:id="@+id/next_btn"
            style="@style/LightRaiseColorButtonRippleStyle"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@drawable/dialog_userauthsuccess_btn_backshape"
            android:gravity="center"
            android:textSize="@dimen/contenttitletext" />
        <com.rey.material.widget.Button
            android:id="@+id/return_btn"
            style="@style/LightRaiseColorButtonRippleStyle"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@drawable/dialog_userauthsuccess_btn_returnshape"
            android:gravity="center"
            android:text=""
            android:visibility="gone"
            android:layout_marginTop="60px"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/contenttitletext" />

    </LinearLayout>


</com.zhy.autolayout.AutoLinearLayout>

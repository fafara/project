<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                       android:layout_width="match_parent"
                                       android:layout_height="match_parent">
	<include
		android:id="@+id/title"
		layout="@layout/ryxpaytitle"
		android:layout_width="match_parent"
		android:layout_height="56dp"
	/>
	<com.zhy.autolayout.AutoRelativeLayout
		android:id="@+id/lay_name"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/title"
		android:layout_marginLeft="70px"
		android:layout_marginRight="47px"
		android:layout_marginTop="74px"
	>
		<EditText
			android:id="@+id/edt_bankname"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_alignParentLeft="true"
			android:background="@null"
			android:cursorVisible="true"
			android:focusable="false"
			android:hint="@string/select_bank"
			android:textColor="@android:color/black"
			android:textColorHint="#cccccc"
			android:textSize="@dimen/bodytextsize"/>

		<ImageView
			android:id="@+id/iv_nameright"
			android:layout_width="25px"
			android:layout_height="48px"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:src="@drawable/righttocontent"/>
	</com.zhy.autolayout.AutoRelativeLayout>
	<View
		android:id="@+id/line_name"
		android:layout_width="match_parent"
		android:layout_height="@dimen/onepx"
		android:layout_below="@+id/lay_name"
		android:layout_marginLeft="70px"
		android:layout_marginTop="42px"
		android:background="@color/register_line_color"/>
	<com.zhy.autolayout.AutoRelativeLayout
		android:id="@+id/lay_province"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/line_name"
		android:layout_marginLeft="70px"
		android:layout_marginRight="47px"
		android:layout_marginTop="52px"
	>
		<EditText
			android:id="@+id/edt_bankprovince"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_alignParentLeft="true"
			android:background="@null"
			android:cursorVisible="true"
			android:focusable="false"
			android:textColorHint="#cccccc"
			android:hint="@string/select_province_bank"
			android:textColor="@android:color/black"
			android:textSize="@dimen/bodytextsize"/>

		<ImageView
			android:id="@+id/iv_provinceright"
			android:layout_width="25px"
			android:layout_height="48px"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:src="@drawable/righttocontent"/>
	</com.zhy.autolayout.AutoRelativeLayout>
	<View
		android:id="@+id/line_province"
		android:layout_width="match_parent"
		android:layout_height="@dimen/onepx"
		android:layout_below="@+id/lay_province"
		android:layout_marginLeft="70px"
		android:layout_marginTop="42px"
		android:background="@color/register_line_color"/>
	<com.zhy.autolayout.AutoRelativeLayout
		android:id="@+id/lay_city"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/line_province"
		android:layout_marginLeft="70px"
		android:layout_marginRight="47px"
		android:layout_marginTop="52px"
	>
		<EditText
			android:id="@+id/edt_bankcity"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_alignParentLeft="true"
			android:background="@null"
			android:cursorVisible="true"
			android:focusable="false"
			android:hint="@string/select_bank_city"
			android:textColorHint="#cccccc"
			android:textColor="@android:color/black"
			android:textSize="@dimen/bodytextsize"/>

		<ImageView
			android:id="@+id/iv_cityright"
			android:layout_width="25px"
			android:layout_height="48px"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:src="@drawable/righttocontent"/>
	</com.zhy.autolayout.AutoRelativeLayout>
	<View
		android:id="@+id/line_city"
		android:layout_width="match_parent"
		android:layout_height="@dimen/onepx"
		android:layout_below="@+id/lay_city"
		android:layout_marginLeft="70px"
		android:layout_marginTop="42px"
		android:background="@color/register_line_color"/>
	<com.zhy.autolayout.AutoRelativeLayout
		android:id="@+id/lay_branchbank"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/line_city"
		android:layout_marginLeft="70px"
		android:layout_marginRight="47px"
		android:layout_marginTop="52px"
	>
		<EditText
			android:id="@+id/edt_bank_branch"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_alignParentLeft="true"
			android:background="@null"
			android:cursorVisible="true"
			android:focusable="false"
			android:hint="@string/select_the_account_where_the_branch"
			android:textColorHint="#cccccc"
			android:textColor="@android:color/black"
			android:textSize="@dimen/bodytextsize"/>

		<ImageView
			android:id="@+id/iv_bank_branchright"
			android:layout_width="25px"
			android:layout_height="48px"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:src="@drawable/righttocontent"/>
	</com.zhy.autolayout.AutoRelativeLayout>
	<View
		android:id="@+id/line_branchbank"
		android:layout_width="match_parent"
		android:layout_height="@dimen/onepx"
		android:layout_below="@+id/lay_branchbank"
		android:layout_marginLeft="70px"
		android:layout_marginTop="42px"
		android:background="@color/register_line_color"/>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/line_branchbank"
		android:layout_marginLeft="90px"
		android:layout_marginRight="90px"
		android:layout_marginTop="90px">

		<com.rey.material.widget.Button
			android:id="@+id/btn_next"
			style="@style/LightRaiseColorButtonRippleStyle"
			android:layout_width="match_parent"
			android:gravity="center"
			android:text="@string/btn_next"
			android:textSize="@dimen/contenttitletext"/>
	</LinearLayout>
</com.zhy.autolayout.AutoRelativeLayout>

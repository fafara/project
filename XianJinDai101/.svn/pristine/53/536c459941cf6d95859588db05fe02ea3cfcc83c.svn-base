        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/ll_hint"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:layout_below="@+id/ll_title">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/six_choose_one"
                android:textColor="#000"
                android:textSize="12sp"
                android:gravity="center"/>
        </com.zhy.autolayout.AutoRelativeLayout>
        <!--人行征信查询-->
        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/xjd_ll_quota_activation_credit"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:layout_marginLeft="30px"
            android:layout_marginRight="30px"
            android:layout_below="@+id/ll_hint"
            android:background="#fff">

            <ImageView
                android:id="@+id/xjd_iv_quota_activation_credit"
                android:layout_width="60px"
                android:layout_height="60px"
                android:background="@drawable/activationcredit"
                android:scaleType="fitCenter"
                android:layout_marginTop="30px"
                android:layout_marginBottom="30px"
                android:layout_marginLeft="30px"/>

            <TextView
                android:id="@+id/tv_quota_activation_credit"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginTop="20px"
                android:layout_marginLeft="48px"
                android:layout_toRightOf="@+id/xjd_iv_quota_activation_credit"
                android:textSize="12sp"
                android:textColor="#000000"
                android:text="@string/people_inquiries"/>
            <ImageView
                android:layout_toRightOf="@+id/tv_quota_activation_credit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20px"
                android:layout_marginLeft="40px"
                android:src="@drawable/recommend"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

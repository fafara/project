                android:layout_height="match_parent"
                android:layout_marginTop="20px"
                android:layout_marginLeft="48px"
                android:layout_toRightOf="@+id/xjd_iv_quota_fund_fnformation_certification"
                android:textSize="12sp"
                android:textColor="#000000"
                android:text="@string/provident_fund_fnformation_certification"/>

            <TextView

                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/xjd_iv_quota_fund_fnformation_certification"
                android:layout_marginLeft="48px"
                android:layout_alignBottom="@+id/xjd_tv_provident_fund_fnformation_certification"
                android:layout_marginTop="20px"
                android:layout_marginBottom="20px"
                android:textSize="10sp"
                android:text="@string/provident_fund_fnformation_certification_language"
                android:textColor="#999999"/>

            <ImageView
                android:layout_width="60px"
                android:layout_height="60px"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15px"
                android:layout_marginTop="15px"
                android:background="@drawable/home_right_img"
                android:scaleType="fitCenter" />
        </com.zhy.autolayout.AutoRelativeLayout>

        <TextView
            android:id="@+id/xjd_ll_quota_fund_fnformation_certification_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/xjd_ll_quota_fund_fnformation_certification"
            android:textColor="#000000"/>


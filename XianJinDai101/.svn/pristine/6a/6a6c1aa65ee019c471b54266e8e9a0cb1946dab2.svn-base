<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ryx_pay_order_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#eef1f3">
    <include layout="@layout/ryxpaytitle"></include>


    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/orderlayout"
        android:orientation="vertical"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="@color/white">

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/layout0"
            android:layout_height="150px"
            android:layout_marginLeft="70px"
            android:layout_width="match_parent"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:gravity="center"
                android:textSize="@dimen/bodytextsize"
                android:textColor="@color/drakblack"
                android:text="订单号"/>
            <TextView
                android:id="@+id/tv_orderid"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_alignParentRight="true"
                android:textSize="@dimen/bodytextsize"
                android:textColor="@color/drakblack"
                android:layout_marginRight="47px" />
        </com.zhy.autolayout.AutoRelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/onepx"
            android:layout_marginLeft="70px"
            android:background="@color/payhorizontalline" />

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/layout1"
            android:layout_height="150px"
            android:layout_marginLeft="70px"
            android:layout_width="match_parent"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:gravity="center"
                android:textSize="@dimen/bodytextsize"
                android:textColor="@color/drakblack"
                android:text="商品名称"/>
            <TextView
                android:id="@+id/tv_productname"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_alignParentRight="true"
                android:textSize="@dimen/bodytextsize"
                android:textColor="@color/drakblack"
                android:layout_marginRight="47px" />
        </com.zhy.autolayout.AutoRelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/onepx"
            android:layout_marginLeft="70px"
            android:background="@color/payhorizontalline" />


        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_height="150px"
            android:layout_marginLeft="70px"
            android:layout_width="match_parent"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:gravity="center"
                android:textSize="@dimen/bodytextsize"
                android:textColor="@color/drakblack"
                android:text="手机号"/>
            <TextView
                android:id="@+id/tv_phonenumber"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_alignParentRight="true"
                android:textSize="@dimen/bodytextsize"
                android:textColor="@color/drakblack"
                android:layout_marginRight="47px" />
        </com.zhy.autolayout.AutoRelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/onepx"
            android:layout_marginLeft="70px"
            android:background="@color/payhorizontalline" />


        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_height="150px"
            android:layout_marginLeft="70px"
            android:layout_width="match_parent"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:gravity="center"
                android:textSize="@dimen/bodytextsize"
                android:textColor="@color/drakblack"
                android:text="订单金额"/>
            <TextView
                android:id="@+id/tv_ordermoney"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_alignParentRight="true"
                android:textSize="@dimen/bodytextsize"
                android:textColor="@color/drakblack"
                android:layout_marginRight="47px" />
        </com.zhy.autolayout.AutoRelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/onepx"
            android:layout_marginLeft="70px"
            android:background="@color/payhorizontalline" />

    </com.zhy.autolayout.AutoLinearLayout>


    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/paywaylayout"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:layout_marginTop="50px">

        <RelativeLayout
            android:layout_width="match_parent"
            android:background="@color/white"
            android:layout_height="150px">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textSize="@dimen/bodytextsize"
                android:gravity="center"
                android:layout_centerInParent="true"
                android:text="选择支付方式"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="@color/DevideBlock"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ryxpay_relayout_1"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:background="@drawable/selector_user_action_bg"
            android:clickable="true"
            >
            <ImageView
                android:id="@+id/ryxpay_iv_1"
                android:layout_width="@dimen/dimen_66_dip"
                android:layout_height="@dimen/dimen_66_dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="70px"
                android:scaleType="centerInside"
                android:src="@drawable/icon_ryxpay_shuka"/>
            <TextView
                android:id="@+id/ryxpay_tv_1"
                style="@style/textViewStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_60_dip"
                android:layout_toRightOf="@id/ryxpay_iv_1"
                android:text="刷卡支付"/>

            <ImageView
                android:layout_width="@dimen/dimen_25_dip"
                android:layout_height="@dimen/dimen_50_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dimen_46_dip"
                android:scaleType="centerInside"
                android:src="@drawable/icon_user_jiantou"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="70px"
                android:layout_marginRight="@dimen/dimen_46_dip"
                android:background="@color/DevideBlock"
                />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/ryxpay_relayout_2"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:background="@drawable/selector_user_action_bg"
            android:clickable="true"
            android:visibility="gone"
            >
            <ImageView
                android:id="@+id/ryxpay_iv_2"
                android:layout_width="@dimen/dimen_66_dip"
                android:layout_height="@dimen/dimen_66_dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_60_dip"
                android:scaleType="centerInside"
                android:src="@drawable/icon_ryxpay_kjzf"/>
            <TextView
                android:id="@+id/ryxpay_tv_2"
                style="@style/textViewStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_60_dip"
                android:layout_toRightOf="@id/ryxpay_iv_2"
                android:text="快捷支付"/>

            <ImageView
                android:layout_width="@dimen/dimen_25_dip"
                android:layout_height="@dimen/dimen_50_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dimen_46_dip"
                android:scaleType="centerInside"
                android:src="@drawable/icon_user_jiantou"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="@color/DevideBlock"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ryxpay_relayout_3"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:background="@drawable/selector_user_action_bg"
            android:clickable="true"
            android:visibility="gone"
            >
            <ImageView
                android:id="@+id/ryxpay_iv_3"
                android:layout_width="@dimen/dimen_66_dip"
                android:layout_height="@dimen/dimen_66_dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_60_dip"
                android:scaleType="centerInside"
                android:src="@drawable/icon_ryxpay_weixin"/>
            <TextView
                android:id="@+id/ryxpay_tv_3"
                style="@style/textViewStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_60_dip"
                android:layout_toRightOf="@id/ryxpay_iv_3"
                android:text="微信二维码支付"/>

            <ImageView
                android:layout_width="@dimen/dimen_25_dip"
                android:layout_height="@dimen/dimen_50_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dimen_46_dip"
                android:scaleType="centerInside"
                android:src="@drawable/icon_user_jiantou"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="@color/DevideBlock"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ryxpay_relayout_4"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:background="@drawable/selector_user_action_bg"
            android:clickable="true"
            android:visibility="gone"
            >
            <ImageView
                android:id="@+id/ryxpay_iv_4"
                android:layout_width="@dimen/dimen_66_dip"
                android:layout_height="@dimen/dimen_66_dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_60_dip"
                android:scaleType="centerInside"
                android:src="@drawable/icon_ryxpay_zhifubao"/>
            <TextView
                android:id="@+id/ryxpay_tv_4"
                style="@style/textViewStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_60_dip"
                android:layout_toRightOf="@id/ryxpay_iv_4"
                android:text="支付宝二维码支付"/>

            <ImageView
                android:layout_width="@dimen/dimen_25_dip"
                android:layout_height="@dimen/dimen_50_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dimen_46_dip"
                android:scaleType="centerInside"
                android:src="@drawable/icon_user_jiantou"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="@color/DevideBlock"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ryxpay_relayout_5"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:background="@drawable/selector_user_action_bg"
            android:clickable="true"
            android:visibility="gone"
            >
            <ImageView
                android:id="@+id/ryxpay_iv_5"
                android:layout_width="@dimen/dimen_66_dip"
                android:layout_height="@dimen/dimen_66_dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_60_dip"
                android:scaleType="centerInside"
                android:src="@drawable/icon_ryxpay_kjzf"/>
            <TextView
                android:id="@+id/ryxpay_tv_5"
                style="@style/textViewStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_60_dip"
                android:layout_toRightOf="@id/ryxpay_iv_5"
                android:text="银联二维码支付"/>

            <ImageView
                android:layout_width="@dimen/dimen_25_dip"
                android:layout_height="@dimen/dimen_50_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dimen_46_dip"
                android:scaleType="centerInside"
                android:src="@drawable/icon_user_jiantou"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="@color/DevideBlock"
                />
        </RelativeLayout>

    </com.zhy.autolayout.AutoLinearLayout>


</com.zhy.autolayout.AutoLinearLayout>

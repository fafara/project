<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.livedetect.FaceCollectSucUtil">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="90px"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/success_img"
                android:layout_width="140dp"
                android:layout_height="186dp"
                android:layout_centerHorizontal="true"
                android:layout_margin="5dp"
                android:background="#ef7c01"
                android:padding="3dp"
                android:scaleType="fitXY" />
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_below="@id/success_img"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:layout_marginLeft="90px"
                android:layout_marginRight="90px"
                android:orientation="horizontal">
            <ImageView
                android:id="@+id/img_success"
                android:layout_width="113px"
                android:layout_height="113px"
                android:scaleType="fitCenter"
                android:background="@drawable/htic_identify_success"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/htjc_success_result"
                android:layout_marginLeft="30px"
                android:visibility="gone"
                android:textColor="#000"
                android:textSize="18sp" />
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoRelativeLayout>
        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/ll_server"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="90px"
            android:layout_marginRight="90px"
            android:orientation="vertical">

            <EditText
                android:id="@+id/et_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="52px"
                android:background="@null"
                android:cursorVisible="true"
                android:hint="姓名"
                android:visibility="gone"
                android:textColor="@android:color/black"
                android:textColorHint="@color/fourblack"
                android:textCursorDrawable="@null" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/onepx"
                android:layout_marginTop="35px"
                android:visibility="gone"
                android:background="@color/register_line_color" />

            <EditText
                android:id="@+id/et_cardid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="52px"
                android:background="@null"
                android:cursorVisible="true"
                android:hint="身份证号"
                android:visibility="gone"
                android:textColor="@android:color/black"
                android:textColorHint="@color/fourblack"
                android:textCursorDrawable="@null" />

            <View
                android:id="@+id/line_name"
                android:layout_width="match_parent"
                android:layout_height="@dimen/onepx"
                android:layout_below="@+id/et_cardid"
                android:layout_marginTop="35px"
                android:visibility="gone"
                android:background="@color/register_line_color" />
            <Button
                android:id="@+id/btn_idenfy"
                style="@style/CLightRaiseColorButtonRippleStyle"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="60px"
                android:text=""
                android:textSize="@dimen/bodytextsize" />

            <Button
                android:id="@+id/btn_server"
                style="@style/CLightRaiseColorButtonRippleStyle"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="60px"
                android:text="激活"
                android:visibility="gone"
                android:textSize="@dimen/bodytextsize" />
        </com.zhy.autolayout.AutoLinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="70dp"
            android:visibility="gone"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/btn_again"
                android:layout_width="111dp"
                android:layout_height="42dp"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:src="@drawable/htjc_btn_again_selector" />

            <ImageView
                android:id="@+id/btn_return"
                android:layout_width="111dp"
                android:layout_height="42dp"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:src="@drawable/htjc_btn_return_selector" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
<resources>
    <style name="CustomProgressDialog" parent="@style/CustomDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>


    <style name="CLightwhiteColorButtonRippleStyle" parent="Material.Drawable.Ripple.Touch.Light">
        <item name="android:background">@drawable/whiteroundshape</item>
        <item name="rd_rippleColor">#33000000</item>
        <item name="rd_leftPadding">1dp</item>
        <item name="rd_topPadding">1dp</item>
        <item name="android:minHeight">56dp</item>
        <item name="rd_rightPadding">1dp</item>
        <item name="rd_bottomPadding">1dp</item>
        <item name="rd_delayClick">afterRelease</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Button</item>
        <item name="android:textColor">@color/drakblack</item>
        <item name="android:layout_height">140dp</item>
    </style>
    <style name="CLightColorButtonRippleStyle" parent="Material.Drawable.Ripple.Touch.Light">
        <item name="android:background">@drawable/whiteroundnoframeshape</item>
        <item name="rd_rippleColor">#33000000</item>
        <item name="rd_leftPadding">1dp</item>
        <item name="rd_topPadding">1dp</item>
        <item name="android:minHeight">56dp</item>
        <item name="rd_rightPadding">1dp</item>
        <item name="rd_bottomPadding">2dp</item>
        <item name="rd_delayClick">afterRelease</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Button</item>
        <item name="android:textColor">@color/drakblack</item>
        <item name="android:layout_height">140dp</item>
    </style>

    <style name="CLightRaiseColorButtonRippleStyle"  parent="Material.Drawable.Ripple.Touch.Light">
        <item name="rd_rippleColor">#33000000</item>
        <item name="rd_leftPadding">1dp</item>
        <item name="rd_topPadding">1dp</item>

        <item name="rd_rightPadding">1dp</item>
        <item name="rd_bottomPadding">2dp</item>
        <item name="rd_delayClick">afterRelease</item>
        <item name="android:background">@drawable/roundshape</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:maxHeight">56dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/btntext</item>

    </style>

    <style name="LeadingDotsStyle" parent="@style/Widget.AppCompat.EditText">

        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/verysmallsize</item>
        <item name="android:background">@drawable/c_dot_selecter</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginRight">10dp</item>

    </style>
    <style name="BlackHorizontalLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/grey_bg</item>
    </style>
    <style name="BlackVerticalLine">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/grey_bg</item>
    </style>
    <style name="LightFlatWaveColorButtonRippleStyle" parent="Material.Drawable.Ripple.Wave.Light">
        <item name="android:background">@null</item>
        <item name="rd_rippleColor">#33000000</item>
        <item name="rd_cornerRadius">4dp</item>
        <item name="rd_delayClick">afterRelease</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Button</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>
    <style name="LightRaiseColorButtonRippleStyle" parent="Material.Drawable.Ripple.Touch.Light">
        <item name="android:background">@drawable/roundshape</item>
        <item name="rd_rippleColor">#33000000</item>
        <item name="rd_leftPadding">1dp</item>
        <item name="rd_topPadding">1dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="rd_rightPadding">1dp</item>
        <item name="rd_bottomPadding">2dp</item>
        <item name="rd_delayClick">afterRelease</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Button</item>
        <item name="android:textColor">@color/abc_primary_text_material_dark</item>
        <item name="android:layout_height">50px</item>
    </style>
    <!--借款内容左标题-->
    <style name="BorrowLeftTitleStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/secondblack</item>
        <item name="android:textSize">@dimen/verysmallsize</item>
    </style>
    <!--确认借款内容-->
    <style name="SureBorrow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SureBorrow.RelativeLayoutStyle">
        <item name="android:paddingBottom">12dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingLeft">24dp</item>
        <item name="android:paddingRight">24dp</item>
    </style>
    <style name="SureBorrow.ContentTitle">
        <item name="android:textSize">@dimen/bodylittletextsize</item>
        <item name="android:textColor">@color/threeblack</item>
    </style>
    <style name="SureBorrow.Content">
        <item name="android:textSize">@dimen/bodylittletextsize</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="mysliderStyle" parent="Material.Widget.Slider.Discrete"></style>
    <style name="LightCheckBoxDrawable" parent="Material.Drawable.CheckBox">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Body1</item>
        <item name="android:textColor">#FF000000</item>
    </style>
    <!--借款记录-->
    <style name="MyTabLayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/contenttitletext</item>

    </style>
    <style name="SimpleDialogLight" parent="Material.App.Dialog.Simple.Light">
        <item name="di_inAnimation">@anim/anim_scale_in</item>
        <item name="di_outAnimation">@anim/anim_scale_out</item>
    </style>
    <style name="SimpleDialog" parent="Material.App.Dialog.Simple">
        <item name="di_inAnimation">@anim/anim_scale_in</item>
        <item name="di_outAnimation">@anim/anim_scale_out</item>
    </style>
    <style name="SimpleNewDialogLight" parent="Material.App.Dialog.Simple.Light">
        <item name="di_inAnimation">@anim/anim_scale_in</item>
        <item name="di_outAnimation">@anim/anim_scale_out</item>
        <item name="di_titleTextColor">@color/blue</item>
        <item name="di_backgroundColor">@color/fifthblack</item>
        <item name="di_titleTextAppearance">@style/SimpleNewDialogTitleLight</item>
    </style>

    <style name="SimpleNewDialogTitleLight" parent="Material.App.Dialog.Simple.Light">
        <item name="android:textSize">@dimen/mediumtitletext</item>
        </style>

    <style name="LightRadioButtonDrawable" parent="Material.Drawable.RadioButton">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Body1</item>
        <item name="android:textColor">#FF000000</item>
    </style>
	<style name="LightFlatColorButtonRippleStyle" parent="Material.Drawable.Ripple.Touch.Light">
		<item name="android:background">@null</item>
		<item name="rd_backgroundColor">#3303A9F4</item>
		<item name="rd_rippleColor">#3303A9F4</item>
		<item name="rd_cornerRadius">0dp</item>
		<item name="rd_delayClick">afterRelease</item>
		<item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Button</item>
		<item name="android:textColor">@color/colorPrimary</item>
	</style>

	<style name="textViewStyle">
		<item name="android:textSize">@dimen/bodytextsize</item>
		<item name="android:textColor">@color/secondblack</item>
	</style>
    <style name="LightCheckBox" parent="Material.Drawable.CheckBox">
        <item name="cbd_width">19dp</item>
        <item name="cbd_height">19dp</item>
        <item name="cbd_boxSize">17dp</item>
        <item name="cbd_cornerRadius">0dp</item>
        <item name="cbd_strokeSize">1dp</item>
        <item name="cbd_animDuration">@android:integer/config_mediumAnimTime</item>
    </style>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <item name="colorPrimary">@color/white</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="AnimActivityTheme">
        <item name="android:windowAnimationStyle">@style/FeelyouWindowAnimTheme</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="FeelyouWindowAnimTheme" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/htjc_in_from_right</item>
        <item name="android:activityOpenExitAnimation">@anim/htjc_out_from_left</item>
        <item name="android:activityCloseEnterAnimation">@anim/htjc_in_from_left</item>
        <item name="android:activityCloseExitAnimation">@anim/htjc_out_from_right</item>
    </style>


    <!-- 下载进度条样式 -->
    <style name="progressBarHorizontal" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:minHeight">5dp</item>
        <item name="android:maxHeight">60dp</item>
    </style>
    <!--下拉框样式-->
    <style name="cmaterialSpinner" parent="Material.Widget.Spinner">
        <item name="spn_labelTextColor">@color/black</item>
    </style>
    <style name="LightSpinnerItem" parent="Material.Drawable.Ripple.Wave">
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">0dp</item>
        <item name="rd_delayClick">afterRelease</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Body1</item>
        <item name="android:textColor">@color/abc_primary_text_material_light</item>
    </style>
    <style name="DarkSpinnerItem" parent="Material.Drawable.Ripple.Wave">
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">0dp</item>
        <item name="rd_delayClick">afterRelease</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Body1</item>
        <item name="android:textColor">@color/abc_primary_text_material_dark</item>
    </style>

    <style name="LightSpinner" parent="Material.Widget.Spinner.Light">
        <item name="rd_style">@style/Material.Drawable.Ripple.Wave.Light</item>
        <item name="spn_labelEnable">true</item>
        <item name="spn_arrowSwitchMode">true</item>
        <item name="spn_arrowAnimDuration">@android:integer/config_shortAnimTime</item>
        <item name="spn_arrowInterpolator">@android:anim/decelerate_interpolator</item>
        <item name="android:popupBackground">@drawable/abc_popup_background_mtrl_mult</item>
    </style>
    <style name="DarkSpinner" parent="Material.Widget.Spinner">
        <item name="rd_style">@style/Material.Drawable.Ripple.Wave</item>
        <item name="spn_labelEnable">true</item>
        <item name="spn_arrowSwitchMode">true</item>
        <item name="spn_arrowAnimDuration">@android:integer/config_shortAnimTime</item>
        <item name="spn_arrowInterpolator">@android:anim/decelerate_interpolator</item>
        <item name="spn_arrowColor">@color/colorAccent</item>
    </style>
    <array name="spinner_item">
        <item>@style/LightSpinnerItem</item>
        <item>@style/DarkSpinnerItem</item>
    </array>
</resources>

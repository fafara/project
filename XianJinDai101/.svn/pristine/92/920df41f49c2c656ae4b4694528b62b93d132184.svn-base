<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff">

    <ImageView
        android:id="@+id/bankimgview"
        android:layout_width="60px"
        android:layout_height="60px"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="50px"
        android:layout_centerVertical="true"
        android:src="@drawable/bank_default" />

    <TextView
        android:id="@+id/banknametextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="42px"
        android:layout_marginLeft="21px"
        android:layout_marginTop="45px"
        android:layout_toRightOf="@id/bankimgview"
        android:textSize="@dimen/bodytextsize" />

    <TextView
        android:id="@+id/tv_paybankcards_account"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="42px"
        android:layout_marginLeft="21px"
        android:layout_marginTop="45px"
        android:layout_toRightOf="@id/banknametextview"
        android:textSize="@dimen/bodytextsize" />

    <TextView
        android:id="@+id/tv_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="42px"
        android:layout_marginRight="30px"
        android:layout_marginTop="45px"
        android:text="暂不支持"
        android:textSize="@dimen/bodytextsize"
        android:visibility="gone" />
    <TextView
        android:id="@+id/tv_credit_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="42px"
        android:layout_marginRight="30px"
        android:layout_marginTop="45px"
        android:text="信用卡"
        android:textSize="@dimen/bodytextsize"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_debit_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="42px"
        android:layout_marginRight="30px"
        android:layout_marginTop="45px"
        android:text="储蓄卡"
        android:textSize="@dimen/bodytextsize"
        android:visibility="gone" />
</com.zhy.autolayout.AutoRelativeLayout>
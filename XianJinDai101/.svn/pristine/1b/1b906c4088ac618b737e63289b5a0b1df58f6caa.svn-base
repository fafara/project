<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ryx.ryxcredit.newfragment.baseinfo.TaoBaoFragment">

    <com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/grey_bg"
        android:orientation="vertical">

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@color/grey_bg"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_login_report"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/c_sixchooseone_taobao"/>
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <com.zhy.autolayout.AutoRelativeLayout
                    android:id="@+id/xjd_rl_sixchooseone_taobaoaccount_login"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="true">
                        <TextView
                            android:id="@+id/xjd_tv_sixchooseone_taobaoaccount_login"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#1db7f0"
                            android:text="@string/c_sixchooseone_taobaoaccount_login"/>

                        <TextView
                            android:id="@+id/xjd_tv_sixchooseone_taobaoaccount_line"
                            android:layout_width="match_parent"
                            android:layout_height="5px"
                            android:layout_below="@+id/xjd_tv_sixchooseone_taobaoaccount_login"
                            android:layout_marginTop="20px"
                            android:background="#1db7f0"/>
                </com.zhy.autolayout.AutoRelativeLayout>
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <com.zhy.autolayout.AutoRelativeLayout
                    android:id="@+id/xjd_rl_sixchooseone_taobaophonenum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="true">
                        <TextView
                            android:id="@+id/xjd_tv_sixchooseone_taobaophonenum_login"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#000000"
                            android:text="@string/c_sixchooseone_taobaophonenum_login"/>

                        <TextView
                            android:id="@+id/xjd_tv_sixchooseone_taobaophonenum_line"
                            android:layout_width="match_parent"
                            android:layout_height="5px"
                            android:layout_below="@+id/xjd_tv_sixchooseone_taobaophonenum_login"
                            android:layout_marginTop="20px"
                            android:background="#4c4c4c"/>
                </com.zhy.autolayout.AutoRelativeLayout>
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/content"
            android:layout_width="match_parent"
            android:layout_above="@id/footer"
            android:layout_height="wrap_content"
            android:layout_below="@+id/xjd_sixchooseone_taobao_content"
            android:orientation="vertical">
            <FrameLayout
                android:id="@+id/xjd_sixchooseone_taobao"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            </FrameLayout>
        </com.zhy.autolayout.AutoLinearLayout>

    </com.zhy.autolayout.AutoLinearLayout>

</FrameLayout>

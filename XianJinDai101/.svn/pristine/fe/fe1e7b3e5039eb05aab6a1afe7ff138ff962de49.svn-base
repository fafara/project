public class PickerBean {
    public PickerBean(String id, String code, String name) {
        this.id = id;
        this.code = code;
        this.name = name;
    }

    public String id;
    public String code;
    public String name;
    public String getPickerViewText() {
        //这里还可以判断文字超长截断再提供显示
        return name;
    }
}

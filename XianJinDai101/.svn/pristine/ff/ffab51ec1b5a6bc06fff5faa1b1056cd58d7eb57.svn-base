<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/grey_bg"
	android:orientation="vertical">
	<include
		android:id="@+id/c_credit_home_header"
		layout="@layout/c_widget_title"/>
	<com.zhy.autolayout.AutoLinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="#eef1f3"
		android:orientation="vertical">
		<!--关于瑞卡贷 -->
		<com.zhy.autolayout.AutoRelativeLayout
			android:id="@+id/rl_problem_about_rqd"
			android:layout_width="match_parent"
			android:layout_height="150px"
			android:layout_marginTop="60px"
			android:background="@drawable/selector_user_action_bg"
			android:clickable="true"
		>
			<TextView
				style="@style/textViewStyle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_marginLeft="40px"
				android:text="关于瑞卡贷"/>

			<ImageView
				android:layout_width="25px"
				android:layout_height="54px"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="46px"
				android:scaleType="centerInside"
				android:src="@drawable/icon_user_jiantou"/>

		</com.zhy.autolayout.AutoRelativeLayout>
		<View
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:background="#e0e0e0"
		/>
		<!--额度问题-->
		<com.zhy.autolayout.AutoRelativeLayout
			android:id="@+id/rl_problem_edu"
			android:layout_width="match_parent"
			android:layout_height="150px"
			android:background="@drawable/selector_user_action_bg"
			android:clickable="true"
		>
			<TextView
				style="@style/textViewStyle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_marginLeft="40px"
				android:text="瑞卡贷额度问题"/>

			<ImageView
				android:layout_width="25px"
				android:layout_height="54px"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="46px"
				android:scaleType="centerInside"
				android:src="@drawable/icon_user_jiantou"/>
		</com.zhy.autolayout.AutoRelativeLayout>
		<View
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:background="#e0e0e0"
		/>
		<!--借款问题-->
		<com.zhy.autolayout.AutoRelativeLayout
			android:id="@+id/rl_problem_borrow"
			android:layout_width="match_parent"
			android:layout_height="150px"
			android:background="@drawable/selector_user_action_bg"
			android:clickable="true"
		>
			<TextView
				style="@style/textViewStyle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_marginLeft="40px"
				android:text="瑞卡贷借款问题"/>

			<ImageView
				android:layout_width="25px"
				android:layout_height="54px"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="46px"
				android:scaleType="centerInside"
				android:src="@drawable/icon_user_jiantou"/>
		</com.zhy.autolayout.AutoRelativeLayout>
		<View
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:background="#e0e0e0"
		/>
		<!-- 还款问题-->
		<com.zhy.autolayout.AutoRelativeLayout
			android:id="@+id/rl_problem_repayment"
			android:layout_width="match_parent"
			android:layout_height="150px"
			android:background="@drawable/selector_user_action_bg"
			android:clickable="true"
		>
			<TextView
				style="@style/textViewStyle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_marginLeft="40px"
				android:text="瑞卡贷还款问题"/>

			<ImageView
				android:layout_width="25px"
				android:layout_height="54px"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="46px"
				android:scaleType="centerInside"
				android:src="@drawable/icon_user_jiantou"/>
		</com.zhy.autolayout.AutoRelativeLayout>
		<View
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:background="#e0e0e0"
			/>
		<!-- 还款问题-->
		<com.zhy.autolayout.AutoRelativeLayout
			android:id="@+id/rl_parameter_repayment"
			android:layout_width="match_parent"
			android:layout_height="150px"
			android:background="@drawable/selector_user_action_bg"
			android:clickable="true"
			>
			<TextView
				style="@style/textViewStyle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_marginLeft="40px"
				android:text="瑞卡贷还款参数问题"/>

			<ImageView
				android:layout_width="25px"
				android:layout_height="54px"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="46px"
				android:scaleType="centerInside"
				android:src="@drawable/icon_user_jiantou"/>
		</com.zhy.autolayout.AutoRelativeLayout>
	</com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>